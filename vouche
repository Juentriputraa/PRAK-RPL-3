public class Voucher {

    private String kodeVoucher;

    public Voucher(long nominal) {
        if (nominal <= 0) {
            this.kodeVoucher = "VC";
        } else {
            this.kodeVoucher = "VC" + nominal;
        }
    }

    public String getKodeVoucher() {
        return kodeVoucher;
    }

}
